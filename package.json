{
  "name": "website-react",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "@researchgate/react-intersection-observer": "^1.0.1",
    "postcss-cli": "^6.1.2",
    "postcss-custom-properties": "^9.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "CI=true react-scripts test",
    "renault": "node ./src/scripts/renault.js https://one-site-review.wired-aws.com",
    "prettier": "prettier --write src/**/*.*",
    "convert": "node ./src/scripts/convertimg.js",
    "postbuild": "react-snap && postcss --no-map --use postcss-custom-properties --replace build/**/*.css; node ./src/htaccess.js"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "husky": "^2.6.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "react-snap": "^1.23.0",
    "react-test-renderer": "^16.9.0",
    "sharp": "^0.22.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/react": "16.9.0",
    "**/react-dom": "16.9.0",
    "**/react-test-renderer": "16.9.0"
  }
}
